        line35x[2] = addressLine1;  &#xD;
		line35x[3] = country;&#xD;
	}&#xD;
	else  &#xD;
        line35x[2] = country;   &#xD;
}  		&#xD;
else&#xD;
{&#xD;
    line35x[0] = name;&#xD;
	if (addressLine1 != null &amp;&amp; addressLine1.length() &gt; 0) &#xD;
	{&#xD;
        line35x[1] = addressLine1; &#xD;
        if (line35x[1].length() &gt; 0)&#xD;
        {&#xD;
            line35x[2] = addressLine2;  &#xD;
            line35x[3] = country;&#xD;
        }&#xD;
        else   &#xD;
            line35x[2] = country;  &#xD;
   	}&#xD;
  	else &#xD;
		line35x[1] = country;   &#xD;
} &#xD;
&#xD;
return line35x;</code>
